{
  "expo": {
    "name": "DOKLINK",
    "slug": "DOKLINK",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "@/assets/images/doklink-image.jpg",
    "scheme": "doklink",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "@/assets/images/doklink-image.jpg",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "@/assets/images/doklink-image.jpg"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "@/assets/images/doklink-image.jpg",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
